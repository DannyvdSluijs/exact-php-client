{
    "name": "picqer/exact-php-client",
    "type": "library",
    "description": "A PHP Client for the Exact API",
    "keywords": [
        "api",
        "php",
        "exact"
    ],
    "homepage": "http://github.com/picqer/exact-php-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Groen",
            "email": "info@picqer.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "guzzlehttp/guzzle": "~6.0|~7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Picqer\\": "src/Picqer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Picqer\\Tests\\": "tests/"
        }
    }
}
